            function doinline() 
            {

                $('input[title]').each(function () {
                    if ($(this).val() === '') { $(this).val($(this).attr('title')); }

                    $(this).focus(function () {
                        if ($(this).val() === $(this).attr('title')) { $(this).val(''); }
                    });

                    $(this).blur(function () {
                        if ($(this).val() === '') { $(this).val($(this).attr('title')); }
                    });
                });


                $('textarea[title]').each(function () {
                    if ($(this).val() === '') { $(this).val($(this).attr('title')); }

                    $(this).focus(function () {
