                            function showsocialbox(m){

                                    //grab the href value and explode the bar symbol to grab the url and title
                                    var value = $(m).attr('url');

                                    //get the parent of the link
                                    m = $(m).parents('.result')[0];

                                    var top = 0;
                                    var left = 0;
                                    if(m)
                                    {
                                    top = $(m).offset().top + 15;
                                    left = $(m).offset().left + 15;
                                    }
                                    else
                                    {
                                     top = $('#window').offset().top + 15 + 20;
                                     left = $('#window').offset().left + 15;
                                     var width = $('#window').width() - 30;
                                     
                                     $('#shareit-box').width(width);
                                    }
                                    
                                    var height = 20;



                                    if (top != $('#shareit-box').offset().top && left != $('#shareit-box').offset().left) { inside = 0; }



                                    //assign the value to variables and encode it to url friendly
                                    var field = value;
                                    var url = encodeURIComponent(value);


                                    //assign the height for the header, so that the link is cover
                                    $('#shareit-header').height(height);

                                    //set the position, the box should appear under the link and centered
                                    $('#shareit-box').css({ 'top': top, 'left': left });

                                    //assign the url to the textfield
                                    $('#shareitfield').val(field);

                                    //display the box
                                    $('#shareit-box').fadeIn('slow');


                                    if (inside == 0) {
                                        $('#socialloading').show();

                                        $('#shareframe').attr('src', '../../socialframe?url=' + url);
                                        $("#shareframe").load(function () {
                                            $('#socialloading').hide();
                                            $("#shareframe").show();
                                        });

                                        inside = 1;
                                    }





                                    //onmouse out hide the shareit box
                         
                                $('#shareit-box-close').click(function () {
                                    $('#shareitfield').val('');
                                    $("#shareframe").hide();
                                    $("#shareit-box").hide();
                                    inside = 0;
                                });

                                
                                //fixes share link bug of reloading through ajax
                                if (once == 0) {
                                    $('a[rel=shareit], #shareit-box').mouseenter(function () {
                                        once = 1;
                                        showsocialbox(this);
                                    });
                                }

                                //hightlight the textfield on click event
                                $('#shareitfield').click(function () {
                                    $(this).select();
                                });
                            
                            }

