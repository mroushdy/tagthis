{
        position: fixed;
    text-align: center;
    top: 30%;
    width: 100%;
       z-index: 100;
    }

#MainLoading div
{
    background-color: rgb(0, 0, 0);
    background-color: rgba(0, 0, 0, 0.8);
    border-radius: 5px 5px 5px 5px;
    -moz-border-radius: 5px;
    -webkit-border-radius: 5px;
    -khtml-border-radius: 5px;
    display: inline-block;
    padding: 6px;
    }
    
    #MainLoading span
    {
        color:White; font-weight:bold; font-family: Helvetica Neue, Helvetica, Arial, Lucida Grande, sans-serif; 
        }
        
    /* add box*/    
    #p-t-box .textboxlist-autocomplete-result
    {
        height: 30px;
        vertical-align:middle;
    }
   
    #p-t-box .textboxlist-autocomplete-result span
    {
        position:relative;
        top:3px;
    }

    #p-t-box .textboxlist-autocomplete-result img
    {
        float: left;
        height: 27px;
        margin: 2px;
        padding-right: 8px;
    }


    /*buttons*/

    .greybutton {
    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#e0e0e0), color-stop(50%,#e0e0e0), color-stop(50%,#d3d3d3), color-stop(100%,#d3d3d3));
    background: -moz-linear-gradient(center top, #e0e0e0 0%, #e0e0e0 50%, #d3d3d3 50%, #d3d3d3 100%);
    -webkit-border-radius: 6px;
    -moz-border-radius: 6px;
    border-radius: 6px;
    padding: 5px 20px;
    display: inline-block;
    font-family: Helvetica Neue, Helvetica;
    font-size: 16px;
    color: #666666;
    font-weight: bold;
    border:1px solid #ccc;
}
.greybutton:hover {
    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#f06196), color-stop(50%,#ef6196), color-stop(50%,#ee4d88), color-stop(100%,#ee4d88));
    background: -moz-linear-gradient(center top, #f06196 0%, #ef6196 50%, #ee4d88 50%, #ee4d88 100%);
    color: #FFFFFF;
    border:1px solid #C94073;
}


.bluebutton {
    background: #239acd;
    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#069eec), color-stop(50%,#069eec), color-stop(50%,#0591DA), color-stop(100%,#0591DA));
    background: -moz-linear-gradient(center top, #069eec 0%, #069eec 50%, #0591DA 50%, #0591DA 100%);
    -webkit-border-radius: 6px;
    -moz-border-radius: 6px;
    border-radius: 6px;
    text-shadow: 1px 1px 0px #4d4d4d;
    padding: 5px 20px;
    display: inline-block;
    border-color: #0287Cd;
    border-width: 1px;
    border-style: solid;
    font-family: Helvetica;
    font-size: 16px;
    color: #ffffff;
    font-weight: bold;
}
.bluebutton:hover {
    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#f06196), color-stop(50%,#ef6196), color-stop(50%,#ee4d88), color-stop(100%,#ee4d88));
    background: -moz-linear-gradient(center top, #f06196 0%, #ef6196 50%, #ee4d88 50%, #ee4d88 100%);
    color: #FFFFFF;
    border:1px solid #C94073;
}


.greenbutton {
    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#63d773), color-stop(50%,#63d773), color-stop(50%,#37c353), color-stop(100%,#37c353));
    background: -moz-linear-gradient(center top, #63d773 0%, #63d773 50%, #37c353 50%, #37c353 100%);
    -webkit-border-radius: 6px;
    -moz-border-radius: 6px;
    border-radius: 6px;
    text-shadow: 1px 1px 0px #5c5a5c;
    padding: 5px 20px;
    display: inline-block;
    border-color: #529f4e;
    border-width: 1px;
    border-style: solid;
    font-family: Helvetica Neue;
    font-size: 16px;
    color: #ffffff;
    font-weight: bold;
}
.greenbutton:hover {
    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#f06196), color-stop(50%,#ef6196), color-stop(50%,#ee4d88), color-stop(100%,#ee4d88));
    background: -moz-linear-gradient(center top, #f06196 0%, #ef6196 50%, #ee4d88 50%, #ee4d88 100%);
    color: #FFFFFF;
    border:1px solid #C94073;
}

.darkgreybutton {
    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#3e3e3e), color-stop(50%,#3e3e3e), color-stop(50%,#262626), color-stop(100%,#262626));
    background: -moz-linear-gradient(center top, #3e3e3e 0%, #3e3e3e 50%, #262626 50%, #262626 100%);
    -webkit-border-radius: 6px;
    -moz-border-radius: 6px;
    border-radius: 6px;
    text-shadow: 1px 1px 0px #080808;
    padding: 5px 20px;
    display: inline-block;
    border-color: #000000;
    border-width: 1px;
    border-style: solid;
    font-family: Helvetica Neue;
    font-size: 16px;
    color: #ffffff;
    font-weight: bold;
}
.darkgreybutton:hover {
    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#f06196), color-stop(50%,#ef6196), color-stop(50%,#ee4d88), color-stop(100%,#ee4d88));
    background: -moz-linear-gradient(center top, #f06196 0%, #ef6196 50%, #ee4d88 50%, #ee4d88 100%);
    color: #FFFFFF;
    border:1px solid #C94073;
}

