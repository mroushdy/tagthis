<body style="margin:0px;">
<div id="fb-root"></div>


<div id="redbar" class="clearfix">
<div id="pageHead" class="clearfix">

<h1 id="pageLogo">
<a title="Home" href="<%= Url.Action("Index","Home")%>" class="ajax"></a>
</h1>



<div id="headNav" class="clearfix">

<!--- start search --->
          
             <% using (Html.BeginForm("Find", "Search", FormMethod.Post, new { id = "srch-form" }))
                {%>

<div id="SearchBox" style="width:300px; float:left; display:block">
<div id="search" style="width:300px; position:absolute; float:left; margin-top:2px;"><input type="text" name="Simple" id="Simple"/></div>

<div id="SearchBoxInfo" class="invisible">
        <input type="hidden" value="everyone" id="SearchWhat" name="SearchWhat" />
        <input type="hidden" value="now" id="time" name="time" />
        <input type="hidden" value="popular" id="sort" name="sort" />
</div>

<div style="float:right; position:relative; left:-6px; top:8px; width:15px;">
        <input type="submit" value="" class="button" id="srch-submit" style="background:transparent url(../../content/images/dropdown/searchb.png) no-repeat; width:15px; border:none; cursor:pointer;" />  
 </div>
 </div>

 <% } %>
 


        <!---- end search --->

       <div style="float:right; display:block; margin-top:8px;"> 
       <% Html.RenderPartial("LogOnUserControl"); %>
       </div>
</div>


</div>
</div>




<div id="content" style="margin:0;" class="content clearfix">



    <%if (!Request.IsAuthenticated) {%>
<div id="logon" style="background:transparent url(../../content/landingpix/ss3.jpg) no-repeat center; position:relative; top:44px; height:250px; float:left; width:100%;">
<div style="position:relative; top:60px; text-align:center;">
<div style="font:normal normal 600 64px/95% Open Sans, Helvetica Neue, Helvetica, Arial, Lucida Grande; letter-spacing:-2px; color:#ffffff;">DISCOVER MUSIC TOGETHER.</div>
<div id="connectbuttons" style="margin-top:30px;">
<script>
    //initializing API
    window.fbAsyncInit = function () {
        FB.init({ appId: '<%= Facebook.FacebookApplication.Current.AppId%>', status: true, cookie: true,
            xfbml: true
        });
    };
    (function () {
        var e = document.createElement('script'); e.async = true;
        e.src = document.location.protocol +
              '//connect.facebook.net/en_US/all.js';
        document.getElementById('fb-root').appendChild(e);
    } ());



    //your fb login function
    function fblogin() {
        FB.login(function (response) {
            window.location = '<%=Url.Action("GetFb","account")%>';
        }, { perms: 'email, publish_actions' });
    }
</script>

<a id="fbcon" href="javascript:void(0);" onclick="fblogin();return false;"></a>
</div>
</div>
</div>
    <%} %>


<div id="contentCol" class="clearfix">
<asp:ContentPlaceHolder ID="MainContent" runat="server" />
</div>

<div id="MainLoading" class="invisible"><div><span>Loading</span><img alt="loading" style=" margin-left: 8px;" src="../../content/icons/Loading_Sprite.gif" /></div></div>




<div id="playerCol" style="background-color:#333333; position:fixed; width:0px;">
<div id="playerwithoutplaylist">
<div id="playertitle" style="font-family:Helvetica, Arial; font-weight:bold; font-size:16px; color:White; text-shadow: rgba(0, 0, 0, 0.496094) 0px 0.08em 0px;  padding:15px 10px; background-color:#1a1a1a; border-bottom:1px solid #000000;"></div>
		<!-- player -->
		<div id="player" class="clearfix" style="border-top:1px solid #151515;">
			<div id="window">
				<div id="flash" style="position:absolute; z-index:-1;"></div>
                <div id="seekbar"><div id="elapsed"></div></div>
                <div class="playcontrol"><span class="play playpause"></span><span class="previous"></span><span class="next"></span><span style="padding: 50px 48px 0 0;" class="mute muteunmute"></span></div>
			    <div class="playbuttons"> 
            
            <span>

              <%if (Request.IsAuthenticated)
              {%>
            <a class="BlackButton CommentButton ajax" href="<%= Url.Action("0","Post")%>" data-pid="r.page.page_id" data-postid="r.pageOwner.PostId">
            Comment
            </a>

            <a class="BlackButton LikeButton" href="javascript:void(0);" data-pid="r.page.page_id" data-postid="r.pageOwner.PostId" onclick="Like(this);">
            Like
            </a>

            <a class="BlackButton RepostButton" href="javascript:void(0);" data-url="r.page.url" data-pid="r.page.page_id" data-postid="r.pageOwner.PostId" onclick="repost(this);">
            Repost
            </a>

            <a class="BlackButton ShareButton" rel="shareit" href="javascript:void(0);" url="r.page.url">
            Share
            </a>
                        <%}%>
            </span>            
            </div>

            </div>
		</div>

		<!--/player -->
        <div style="padding:14px 0; font-family:Helvetica, Arial; font-weight:bold; font-size:18px; color:#ffffff; text-shadow: rgba(0, 0, 0, 0.597656) 0px 1px 3px; background: url(../../content/images/nav_bg.png) repeat-x rgb(34, 34, 34); -webkit-box-shadow: rgb(68, 68, 68) 4px 4px 5px; box-shadow: 4px 4px 5px #444444; border-top:1px solid #000000; text-align:center; vertical-align:middle;">Playlist</div>
        </div>
        <div id="playList" style="z-index:1;" class="mousescroll"></div>
        
</div>

</div>






<div id="shareit-box" style="position:absolute;">
<div id="shareit-header">
<span id="shareit-box-close"></span>
</div>
    

    <div id="shareit-body" class="clearfix">
        <div id="shareit-blank"></div>
        <div id="shareit-url"><input type="text" value="" name="shareit-field" id="shareitfield" class="field"/></div>
       
       <div id="socialloading" style="background:url('../../Content/icons/loading29.gif') no-repeat center center; width:130; height:90px; display:none;" ></div>
       
        <div id="shareit-icon" class="clearfix" >
        
        <iframe id="shareframe" src="" width="110" height="98" scrolling="no" frameborder="0" class="invisible" style="margin:-2px 0 0 0;"></iframe>

        </div>
    </div>
    </div>




    


