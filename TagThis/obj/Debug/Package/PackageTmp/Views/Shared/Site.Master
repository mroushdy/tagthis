//link hijax
 $(function () {
     $(".ajax").live("click", function () { var el = this; var url = $(el).attr("href"); DoHijaxing(url); return false;});
     $('#srch-form').submit(function () { addsearchbit(); var el = this; var url = $(el).attr("action") +"?"+ $(this).serialize(); DoHijaxing(url); return false;});
 });



     function DoHijaxing(url) {


        History.pushState(null, null, ("/"+url).toString().replace("//","/"));

        $('#MainLoading').show();

        $.ajax({
            url: url,
            type: 'GET',
            dataType: 'html',
            success: function (result) {
                $('div#contentCol').html(result);
                $('#MainLoading').hide();
            }
        });
    }

//end ajax hijaxing


function Like(element) {
    var postid = $(element).attr('data-postid');
    var pageid = $(element).attr('data-pid');
    var linktext = $(element).text().toString().trim();
    var rating = $('#likes'+postid.toString()).text();
    var r = parseInt(rating);
    if (linktext == "Like") {
        $(element).text('Unlike');
        r = r + 1;
    }
    else {
        $(element).text('Like');
        r = r - 1;
    }
    $('#likes'+postid.toString()).text(r.toString());

    var url = '<%= Url.Action("LHF", "Ajax", new { value = "l"}) %>'+"&pid="+pageid.toString()+"&postid="+postid.toString();
    $.ajax({
        url: url,
        type: 'GET',
        dataType: 'html',
        success: function (result) {}

    });
}

function GetNotifications() {

    if ($('.notifications .container').is(":visible")) {
        setTimeout(GetNotifications, 1000);
    }
    else {
        var url = '<%= Url.Action("GetNotifications", "Ajax", new {}) %>';
        $.ajax({
            url: url,
            type: 'GET',
            dataType: 'html',
            success: function (result) { $('#notificationscontainer').html(result); BlackSroll(); setTimeout(GetNotifications, 30000); }

        });
    }
}


