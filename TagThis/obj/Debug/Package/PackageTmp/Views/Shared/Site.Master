var videoID =""; 

     function attachurl(){

         $("#preurlattach").hide(); $("#preurlloader").show();
         var url = $("#preurl").val();
         var isyoutube = url.match(/((http:\/\/)?(?:youtu\.be\/|(?:[a-z]{2,3}\.)?youtube\.com\/v\/)([\w-]{11}).*|http:\/\/(?:youtu\.be\/|(?:[a-z]{2,3}\.)?youtube\.com\/watch(?:\?|#\!)v=)([\w-]{11}).*)/i);

         if (isyoutube) 
         { 
            videoID = url.match(/v=(.{11})/)[1];
            $("#url").val(url);
                $.ajax({
                        url: "http://gdata.youtube.com/feeds/api/videos/"+ videoID +"?v=2&alt=jsonc",
                        dataType: "jsonp",
                        success: function (data) { parseresults(data); }
                });
         }
         else
         {
            $("#preurlattach").show(); $("#preurlloader").hide();
            $('#preurlerror').text("Currently we only support music from youtube. We will include more websites soon.");
         }
     }

     function parseresults(data) {
         var category = data.data.category;
   
                if(category == "Music") {
                    var title = TextAbstract(data.data.title,120);
                    var description = TextAbstract(data.data.description,200);
                  
                    $('#movietitle').text(title);
                    $('#moviedescription').text(description);
                    $('#moviethumb').attr("src", "http://img.youtube.com/vi/" + videoID + "/1.jpg");
                    $('#preurlcontainer').hide();
                    $('#addform').fadeIn('slow');

                    rePositionModal('AddModal');
                }
                else
                {
                    $("#preurlattach").show(); $("#preurlloader").hide();
                    $('#preurlerror').text("It seems like this youtube video is not music.");
                }
                
        }

        function TextAbstract(text, length) {
            if (text == null) {
                return "";
            }
            if (text.length <= length) {
                return text;
            }
            text = text.substring(0, length);
            last = text.lastIndexOf(" ");
            text = text.substring(0, last);
            return text + "...";
        }


        function rePositionModal(mid) {
            var mheight = $('#'+mid+' .modal').height();
            var bottom = Math.round(mheight / 2);
            var mwidth = $('#'+mid+' .modal').width();
            var left = Math.round(mwidth / 2);
            $('#'+mid+' .modal').css("margin-left", "-" + left.toString() + "px").css("margin-bottom", "-" + bottom.toString() + "px").css("height", mheight.toString()).css("width", mwidth.toString());
        }



        //infinity scroll
        $(window).scroll(function () {

            if ($(window).scrollTop() >= ($(document).height() - (2 * $(window).height()) ))

                if ($(".showmorebutton").length > 0) {

                    $(".showmorebutton a").click();
                }
                else {
                    return false;
                }

        });


</script>

