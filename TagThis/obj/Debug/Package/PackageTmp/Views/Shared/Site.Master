  function masonry() {
        try {

            $('#resultcontent').masonry('destroy');

            $('#resultcontent').masonry({ isFitWidth: true });


            var $container = $('#resultcontent');

            $container.imagesLoaded(function () {
                $container.masonry({ isFitWidth: true });
            });
        }
        catch (err) { }

    }



    function DelayedMasonry() { setTimeout(masonry, 500); }


    function ajaxmasonry() {
    try{
        var $container = $('#resultcontainer');

        $container.imagesLoaded(function () {
            $container.masonry({isFitWidth:true});
        });

        }
        catch(err){}

    }
